{
  "expo": {
    "name": "Showme",
    "slug": "showme",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/showme.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Welcome to the Showme native Beta! This version should run on both iOS and Android, however you will need to download 'Expo Go' from the App/Play store.",
    "githubUrl": "https://github.com/Samwisee/Showme-native"
  }
}
